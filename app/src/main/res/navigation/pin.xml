    app:startDestination="@id/register"
    android:id="@+id/action_pin">


    <fragment
        android:id="@+id/register"
        android:name="com.example.retrofittest.other.pin.Register"
        android:label="Register">

        <action
            android:id="@+id/action_register_to_registered"
            app:destination="@id/registered" />

    </fragment>


    <fragment
        android:id="@+id/registered"
        android:name="com.example.retrofittest.other.pin.Registered"
        android:label="Registered"/>

</navigation>